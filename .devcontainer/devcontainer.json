{
  "name": "C Project (Stable Build)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "containerEnv": {
    "DOTNET_SYSTEM_GLOBALIZATION_INVARIANT": "1"
  },
  "customizations": {
    "jetbrains": {
      "backend": "CLion"
    }
  },
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt", "seccomp=unconfined"
  ],
  "remoteUser": "vscode"
}